   "cell_type": "code",
   "source": [
    "import re\n",
    "\n",
    "file = open('../data/baby1990.html')\n",
    "pattern = r\"\\b(19\\d{2}|20\\d{2})\\b\" # Buscar todos los años que empiecen con 19 o 20\n",
    "\n",
    "for line in file:\n",
    "    years = re.findall(pattern, line)\n",
    "    for year in years:\n",
    "        print(f\"Año: {year}\")\n"
   ],
